{
  "name": "kim",
  "scope": "kims-projects-005a1207",
  "version": 2,
  "builds": [
    {
      "src": "src/index.html",
      "use": "@vercel/static"
    },
    {
      "src": "src/**/*.{js,css,png,jpg,jpeg,gif,svg}",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/$1"
    }
  ],
  "public": true
}